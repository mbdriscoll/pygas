    -I@GASNET_PATH@/include -I@GASNET_PATH@/include/mpi-conduit -I@MPI_PATH@/include \
    -L@GASNET_PATH@/lib -L@MPI_PATH@/lib \
    -lgasnet-mpi-seq -lammpi -lmpich -lpmpich -lmpl

